[
  {
    "context": "Editor || Workspace",
    "bindings": {
      "cmd-shift-g": "git_panel::ToggleFocus",
      "cmd-shift-space": "editor::ShowCompletions",
      "alt-z": "editor::ToggleSoftWrap"
    }
  },
  {
    "context": "Editor && (vim_mode == normal || vim_mode == visual) && !VimWaiting && !menu",
    "bindings": {
      ";": ["workspace::SendKeystrokes", "5 j"],
      ",": ["workspace::SendKeystrokes", "5 k"],
      "d": ["workspace::SendKeystrokes", "\" _ d"],
      "D": ["workspace::SendKeystrokes", "\" _ D"],
      "c": ["workspace::SendKeystrokes", "\" _ c"],
      "C": ["workspace::SendKeystrokes", "\" _ C"],
      "J": "editor::MoveLineDown",
      "K": "editor::MoveLineUp"
    }
  },
  {
    "context": "Editor && vim_mode == normal && !VimWaiting && !menu",
    "bindings": {
      "x": ["workspace::SendKeystrokes", "\" _ x"]
    }
  },
  {
    "context": "Editor && vim_mode == visual && !VimWaiting && !menu",
    "bindings": {
      "<": ["workspace::SendKeystrokes", "< g v"],
      ">": ["workspace::SendKeystrokes", "> g v"]
    }
  },
  {
    "context": "Editor && vim_mode == insert && !menu",
    "bindings": {
    }
  },
  {
    "bindings": {
      // "cmd-]": "editor::GoToDiagnostic", // use g [
      // "cmd-[": "editor::GoToPrevDiagnostic"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-shift-d": [
        "editor::SelectNext",
        {
          "replace_newest": false
        }
      ]
    }
  },
  {
    "context": "ContextEditor > Editor",
    "bindings": {
      "cmd-shift-g": null
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "cmd-shift-g": null
    }
  }
]

